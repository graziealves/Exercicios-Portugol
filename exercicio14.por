programa {
  funcao inicio() {
    cadeia letra
     escreva("Digite uma letra para definir o sexo: ")
     leia(letra)
        se(letra=="f"){
          escreva("Feminino")
        }
        senao
            se(letra=="m"){
              escreva("Masculino")
            }
            senao{
              escreva("Sexo inválido")
            }
    
  }
}
