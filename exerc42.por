programa {
  funcao inicio() {
    real n1,n2, n3,n4,n5,soma,media
    escreva("Digite a sua primeira nota: ")
    leia(n1)
    escreva("Digite a sua segunda: ")
    leia(n2)
    escreva("Digite a sua terceira: ")
    leia(n3)
    escreva("Digite a sua quarta: ")
    leia(n4)
    escreva("Digite a sua quinta: ")
    leia(n5)
    soma=n1+n2+n3+n4+n5
    media=soma/5
    escreva("A soma desses números é: ",soma)
    escreva("\nA média da soma desses números é de: ", media)
    
  }
}
